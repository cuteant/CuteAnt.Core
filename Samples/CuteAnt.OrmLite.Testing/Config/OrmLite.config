<?xml version="1.0" encoding="utf-8"?>
<!--ORM配置-->
<OrmLiteConfig>
  <!--是否启用远程通讯，默认不启用。-->
  <IsORMRemoting>false</IsORMRemoting>
  <!--是否启用调试。默认为不启用。-->
  <IsORMDebug>true</IsORMDebug>
  <!--是否输出SQL语句。默认为不启用。-->
  <IsORMShowSQL>true</IsORMShowSQL>
  <!--跟踪SQL执行时间，大于该阀值将输出日志，默认0毫秒不跟踪。-->
  <TraceSQLTime>100</TraceSQLTime>
  <!--是否启用缓存调试，默认不启用。-->
  <IsCacheDebug>true</IsCacheDebug>
  <!--是否独占数据库，独占时实体缓存、单对象缓存常驻内存，默认true。-->
  <IsCacheAlone>true</IsCacheAlone>
  <!--是否写实体日志，默认启用。-->
  <IsEntityDebug>true</IsEntityDebug>
  <!--是否启用读写锁机制-->
  <ReadWriteLockEnable>true</ReadWriteLockEnable>
  <!--实体类首次访问数据库时，是否执行数据初始化，默认true。-->
  <InitData>true</InitData>
  <!--数据库缓存有效期，默认关闭：【-2】关闭缓存；【-1】非独占数据库，有外部系统操作数据库，使用请求级缓存；【0】永久静态缓存；【>0】静态缓存时间，单位是秒-->
  <CacheExpiration>60</CacheExpiration>
  <!--缓存维护定时器的检查周期，默认5秒-->
  <CacheCheckPeriod>5</CacheCheckPeriod>
  <!--非独占模式下实体缓存过期时间，默认60秒-->
  <EntityCacheExpire>600</EntityCacheExpire>
  <!--实体缓存最大记录数，默认1000条记录；当实体记录总数超过这个阈值，系统会自动清空实体缓存。-->
  <EntityCacheMaxCount>1000</EntityCacheMaxCount>
  <!--非独占模式下单对象缓存过期时间，默认60秒-->
  <SingleCacheExpire>600</SingleCacheExpire>
  <!--是否启用反向工程，默认不启用。反向工程可以实现通过实体类反向更新数据库结构-->
  <NegativeEnable>true</NegativeEnable>
  <!--是否只检查不操作，默认不启用。启用时，仅把更新SQL写入日志-->
  <NegativeCheckOnly>false</NegativeCheckOnly>
  <!--是否启用不删除字段，默认不启用。删除字段的操作过于危险，这里可以通过设为true关闭-->
  <NegativeNoDelete>false</NegativeNoDelete>
  <!--要排除的链接名和表名，多个用逗号分隔，默认空。-->
  <NegativeExclude />
  <!--连接名映射#，表名映射@，把实体类中的Test2和Test3连接名映射到Test去，例如：Test2#Test,Test3#Test,Area@Test。-->
  <ConnMaps />
  <!--针对允许为空且没有默认值的字段，插入数据时是否允许智能识别并添加相应字段的默认数据，默认不启用。-->
  <AllowInsertDataIntoNullableColumn>true</AllowInsertDataIntoNullableColumn>
  <!--文件型数据库存储路径，默认 Data 。-->
  <FileDataBasePath>Data</FileDataBasePath>
  <!--Oracle Dll Path-->
  <OracleDllPath>C:\Oracle</OracleDllPath>
  <!--是否启用动态代码调试，把动态生成的实体类代码和程序集输出到临时目录，默认不启用。-->
  <IsCodeDebug>false</IsCodeDebug>
  <!--是否ID作为id的格式化，否则使用原名。默认使用ID-->
  <IsModelUseID>true</IsModelUseID>
  <!--是否自动去除前缀，第一个_之前。默认启用-->
  <IsModelAutoCutPrefix>true</IsModelAutoCutPrefix>
  <!--是否自动去除字段前面的表名。默认启用-->
  <IsModelAutoCutTableName>true</IsModelAutoCutTableName>
  <!--是否自动纠正大小写。默认启用-->
  <IsModelAutoFixWord>true</IsModelAutoFixWord>
  <!--格式化表名字段名时，要过滤的前缀。默认tbl,table-->
  <ModelFilterPrefixs>tbl,table</ModelFilterPrefixs>
  <!--自定义数据模型的默认连接名，默认Custom-->
  <SpriteDefaultConnName>Custom</SpriteDefaultConnName>
  <!--自定义数据模型的默认命名空间，默认CuteAnt.OrmLite.Custom-->
  <SpriteDefaultNameSpace>HmFramework.Data.Custom</SpriteDefaultNameSpace>
</OrmLiteConfig>