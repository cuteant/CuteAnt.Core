{
  "version": "3.3.0-rc1-151205",
  "description": "Castle Windsor is best of breed, mature Inversion of Control container available for .NET and Silverlight.",
  "authors": [ "Castle Project Contributors" ],
  "tags": [ "castle, windsor, inversionOfControl, DependencyInjection" ],
  "iconUrl": "http://docs.castleproject.org/GetFile.aspx?File=/castleLogo100px.png",
  "projectUrl": "http://docs.castleproject.org/Windsor.MainPage.ashx",
  "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0.html",

  "repository": {
    "type": "git",
    "url": "https://github.com/castleproject/Windsor"
  },

  "compilationOptions": {
    "define": [ "TRACE", "DOTNET" ],
    "keyFile": "../../tools/StrongKey.snk",
    "warningsAsErrors": true,
    "allowUnsafe": false
  },
  "compile": [ "**/*.cs" ],
  "compileExclude": [ ],
  "exclude": [ ],
  "preprocess": [ ],
  "preprocessExclude": [ ],
  "resource": [ "**/*.resx" ],

  "dependencies": {
    "Castle.Core": "3.3.3-rc1-151205"
  },

  "frameworks": {
    "net40": {
      "compilationOptions": { "define": [ "DOTNET40" ] },
      "frameworkAssemblies": {
        "System.configuration": "4.0.0.0",
        "System.Web": "4.0.0.0",
        "System.XML": "4.0.0.0"
      }
    },
    "net451": {
      "compilationOptions": { "define": [ "DOTNET45" ] },
      "frameworkAssemblies": {
        "System.configuration": "4.0.0.0",
        "System.Web": "4.0.0.0",
        "System.XML": "4.0.0.0"
      }
    }
  }
}
