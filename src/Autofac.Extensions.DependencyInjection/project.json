{
  "version": "4.0.0-rc1-151205",
  "authors": [ "Autofac Contributors" ],
  "description": "Autofac implementation of the .NET Framework dependency injection abstraction.",
  "projectUrl": "http://autofac.org",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "iconUrl": "http://code.google.com/p/autofac/logo",
  "requireLicenseAcceptance": false,
  "tags": [ "Autofac", "DI", "IoC", "AspNet", "DNX" ],
  "compilationOptions": {
    "define": [ "TRACE" ],
    "keyFile": "../../tools/StrongKey.snk",
    "warningsAsErrors": true
  },
  "compile": [ "**/*.cs" ],
  "resource": [ "**/*.resx" ],

  "repository": {
    "type": "git",
    "url": "https://github.com/autofac"
  },

  "dependencies": {
    "Autofac": "4.0.0-rc1-151205"
  },

  "frameworks": {
    "dnx451": {
      "compilationOptions": {
        "define": [ "DNXCLR" ]
      },
      "dependencies": {
        "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0-rc1-final"
      }
    },
    "net451": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR" ]
      },
      "dependencies": {
        "CuteAnt.Extensions.DependencyInjection.Abstractions": "2.0.0-rc1-final"
      }
    }
  }
}
