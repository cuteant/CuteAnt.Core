  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE", "ASYNC" ],
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": false,
        "allowUnsafe": true
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "TRACE", "ASYNC" ],
        "optimize": true,
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": false,
        "allowUnsafe": true
      }
    }
  },
  "compile": [
    "**/*.cs"
  ],
  "compileExclude": [ ],
  "exclude": [ ],
  "preprocess": [ ],
  "preprocessExclude": [ ],
