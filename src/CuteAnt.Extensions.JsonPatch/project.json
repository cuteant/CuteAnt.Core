{
  "version": "2.0.0-rc3-160601",
  "description": "CuteAnt.Extensions.JsonPatch Class Library",
  "authors": [ "SeaBiscuit" ],
  "tags": [ "CuteAnt", "CuteAnt.Extensions.JsonPatch" ],
  "projectUrl": "http://www.cuteant.net",
  "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
  "requireLicenseAcceptance": true,

  "repository": {
    "type": "git",
    "url": "http://git.oschina.net/cuteant/"
  },

  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE", "NET_3_5_GREATER" ],
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": true,
        "allowUnsafe": false,
        "nowarn": [ "CS1591" ]
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "TRACE", "NET_3_5_GREATER" ],
        "optimize": true,
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": true,
        "allowUnsafe": false,
        "nowarn": [ "CS1591" ],
        "xmlDoc": true
      }
    }
  },
  "compile": [
    "**/*.cs",
    "../../Common/*.cs"
  ],
  "compileExclude": [ ],
  "exclude": [ ],
  "preprocess": [ ],
  "preprocessExclude": [ ],
  "resource": [ "**/*.resx" ],

  "dependencies": {
    "Newtonsoft.Json": "9.0.1",
    "CuteAnt.Extensions.Reflection.Sources": {
      "version": "2.0.0-rc3-*",
      "type": "build"
    }
  },

  "frameworks": {
    "net40": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR" ]
      }
    },
    "net451": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR", "NET_4_0_GREATER" ]
      }
    }
  }
}
