  "resource": [ "**/*.resx" ],

  "dependencies": {
    "CuteAnt.IdentityModel.Tokens.Jwt": "4.0.0-rc2-*"
  },

  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.IdentityModel": "4.0.0.0",
        "System.identitymodel.services": "4.0.0.0",
        "System.Net.Http": "4.0.0.0",
        "System.Web": "4.0.0.0",
        "System.Web.Extensions": "4.0.0.0"
      }
    }
  },

