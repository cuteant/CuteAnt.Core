  "requireLicenseAcceptance": true,

  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE", "NET_3_5_GREATER", "NET_4_0_GREATER" ],
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": true,
        "allowUnsafe": false
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "TRACE", "NET_3_5_GREATER", "NET_4_0_GREATER" ],
        "optimize": true,
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": true,
        "allowUnsafe": false
      }
    }
  },
  "compile": [
    "**/*.cs",
    "../CuteAnt.IdentityModel.Tokens.Jwt/Common/DateTimeUtil.cs",
