  "preprocessExclude": [ ],
  "resource": [ "**/*.resx" ],

  "dependencies": {
    "CuteAnt.Extensions.Configuration": "2.0.0-rc1-final",
    "CuteAnt.Extensions.Configuration.FileExtensions": "2.0.0-rc1-final"
  },

  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Security": "4.0.0.0",
        "System.XML": "4.0.0.0"
      }
    }
  }
