{
  "version": "2.0.0-rc2-final",
  "description": "CuteAnt.Extensions.DependencyInjection Class Library",
  "authors": [ "SeaBiscuit" ],
  "tags": [ "CuteAnt", "CuteAnt.Extensions.DependencyInjection" ],
  "projectUrl": "http://www.cuteant.net",
  "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
  "requireLicenseAcceptance": true,

  "repository": {
    "type": "git",
    "url": "http://git.oschina.net/cuteant/"
  },

  "compilationOptions": {
    "define": [ "TRACE", "NET_3_5_GREATER" ],
    "keyFile": "../../tools/StrongKey.snk",
    "warningsAsErrors": false,
    "allowUnsafe": false
  },
  "compile": [
    "**/*.cs",
    "../../Common/*.cs"
  ],
  "compileExclude": [ ],
  "exclude": [ ],
  "preprocess": [ ],
  "preprocessExclude": [ ],
  "resource": [ "**/*.resx" ],

  "dependencies": {
    "CuteAnt.Extensions.DependencyInjection.Abstractions": "2.0.0-rc2-final",
    "Nito.AsyncEx.ExceptionEnlightenment.Sources": {
      "version": "2.0.0-rc2-160101",
      "type": "build"
    },
    "CuteAnt.Extensions.Reflection.Sources": {
      "version": "2.0.0-rc2-160101",
      "type": "build"
    }
  },

  "frameworks": {
    "net40": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR" ]
      },
      "dependencies": {
        "Microsoft.Bcl.Build": "1.0.21",
        "Microsoft.Bcl": "1.1.10",
        "Microsoft.Bcl.Async": "1.0.168"
      }
    },
    "net451": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR" ]
      }
    }
  }
}
