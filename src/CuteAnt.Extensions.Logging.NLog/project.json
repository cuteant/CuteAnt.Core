{
  "version": "2.0.0-rc3-160201",
  "description": "CuteAnt.Extensions.Logging.NLog Class Library",
  "authors": [ "SeaBiscuit" ],
  "tags": [ "CuteAnt", "CuteAnt.Extensions.Logging.NLog" ],
  "projectUrl": "http://www.cuteant.net",
  "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
  "requireLicenseAcceptance": true,

  "repository": {
    "type": "git",
    "url": "http://git.oschina.net/cuteant/"
  },

  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE", "NET_3_5_GREATER" ],
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": false,
        "allowUnsafe": false,
        "nowarn": [ "CS1591" ]
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "TRACE", "NET_3_5_GREATER" ],
        "optimize": true,
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": false,
        "allowUnsafe": false,
        "nowarn": [ "CS1591" ],
        "xmlDoc": true
      }
    }
  },
  "compile": [
    "**/*.cs",
    "../../Common/*.cs"
  ],
  "compileExclude": [ ],
  "exclude": [ ],
  "preprocess": [ ],
  "preprocessExclude": [ ],
  "resource": [ "**/*.resx" ],

  "dependencies": {
    "NLog": "4.2.3"
  },

  "frameworks": {
    "net40": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR" ]
      },
      "dependencies": {
        "CuteAnt.Extensions.Logging.Abstractions": "2.0.0-rc3-*"
      },
      "frameworkAssemblies": {
        "System.XML": "4.0.0.0"
      }
    },
    "net451": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR", "NET_4_0_GREATER" ]
      },
      "dependencies": {
        "CuteAnt.Extensions.Logging.Abstractions": "2.0.0-rc3-*"
      },
      "frameworkAssemblies": {
        "System.XML": "4.0.0.0"
      }
    },
    "dnx451": {
      "compilationOptions": {
        "define": [ "DNXCLR", "NET_4_0_GREATER" ]
      },
      "dependencies": {
        "Microsoft.Extensions.Logging.Abstractions": "1.0.0-rc1-final",
        "Microsoft.AspNet.Hosting": "1.0.0-rc1-final"
      },
      "frameworkAssemblies": {
        "System.XML": "4.0.0.0"
      }
    }
  }
}
