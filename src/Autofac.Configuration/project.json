{
  "version": "4.0.0-rc1-151205",
  "authors": [ "Autofac Contributors" ],
  "description": "Configuration support for Autofac",
  "summary": "Enables Autofac dependencies to be set up using configuration sources.",
  "projectUrl": "http://autofac.org",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "iconUrl": "http://code.google.com/p/autofac/logo",
  "requireLicenseAcceptance": false,
  "tags": [ "Autofac", "DI", "IoC" ],

  "compilationOptions": {
    "define": [ "TRACE" ],
    "keyFile": "../../tools/StrongKey.snk",
    "warningsAsErrors": true,
    "allowUnsafe": false
  },
  "compile": [ "**/*.cs" ],
  "compileExclude": [ ],
  "exclude": [ ],
  "preprocess": [ ],
  "preprocessExclude": [ ],
  "resource": [ "**/*.resx" ],

  "dependencies": {
    "Autofac": "4.0.0-rc1-151205",
    "Microsoft.Extensions.Configuration": "1.0.0-rc1-final"
  },

  "frameworks": {
    "dnx451": {
      "compilationOptions": {
        "define": [ "DNXCLR" ]
      },
      "dependencies": {
      }
    },
    "net451": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR" ]
      },
      "dependencies": {
      }
    }
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/autofac/Autofac.Configuration"
  }
}
