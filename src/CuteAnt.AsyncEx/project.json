{
  "version": "2.0.0-rc3-160302",
  "description": "CuteAnt.AsyncEx Class Library",
  "authors": [ "SeaBiscuit" ],
  "tags": [ "CuteAnt", "CuteAnt.AsyncEx" ],
  "projectUrl": "http://www.cuteant.net",
  "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
  "requireLicenseAcceptance": true,

  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE", "NET_3_5_GREATER" ],
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": false,
        "allowUnsafe": true,
        "nowarn": [ "CS1591" ]
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "TRACE", "NET_3_5_GREATER" ],
        "optimize": true,
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": false,
        "allowUnsafe": true,
        "nowarn": [ "CS1591" ],
        "xmlDoc": true
      }
    }
  },
  "compile": [
    "**/*.cs",
    "../../Common/*.cs"
  ],
  "compileExclude": [ ],
  "exclude": [ ],
  "preprocess": [ ],
  "preprocessExclude": [ ],
  "resource": [ "**/*.resx" ],

  "dependencies": {
    "Ix-Main": "1.2.5",
    "Ix-Async": "1.2.5",
    "CuteAnt": "2.0.0-rc3-*",
    "CuteAnt.Extensions.Sources": {
      "type": "build",
      "version": "2.0.0-rc3-*"
    },
    "CuteAnt.Extensions.Reflection.Sources": {
      "type": "build",
      "version": "2.0.0-rc3-*"
    },
    "Nito.AsyncEx.Tasks.Sources": {
      "type": "build",
      "version": "2.0.0-rc3-*"
    },
    "CuteAnt.Extensions.Primitives": "2.0.0-rc3-*"
  },

  "frameworks": {
    "net40": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR", "NONATIVETASKS", "PRENET45" ]
      },
      "dependencies": {
        "Microsoft.Bcl.Build": "1.0.21",
        "Microsoft.Bcl": "1.1.10",
        "Microsoft.Bcl.Async": "1.0.168"
      },
      "frameworkAssemblies": {
        "System.Runtime.Serialization": ""
      }
    },
    "net451": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR", "NET_4_0_GREATER", "FEATURE_TRACING" ]
      },
      "dependencies": {
        "System.Collections.Immutable": "1.1.38-rc1-final"
      },
      "frameworkAssemblies": {
        "System.Runtime.Serialization": "",
        "System.Collections": {
          "type": "build"
        },
        "System.Runtime": {
          "type": "build"
        }
      }
    },
    "dnx451": {
      "compilationOptions": {
        "define": [ "DNXCLR", "NET_4_0_GREATER", "FEATURE_TRACING" ]
      },
      "dependencies": {
        "System.Collections.Immutable": "1.1.38-rc1-final"
      },
      "frameworkAssemblies": {
        "System.Runtime.Serialization": "",
        "System.Collections": {
          "type": "build"
        },
        "System.Runtime": {
          "type": "build"
        }
      }
    }
  },

  "repository": {
    "type": "git",
    "url": "http://git.oschina.net/cuteant/"
  }
}
