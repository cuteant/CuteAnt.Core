{
  "version": "2.0.0-rc2-151206",
  "description": "protobuf-net Class Library",
  "authors": [ "Marc Gravell" ],
  "tags": [ "CuteAnt", "CuteAnt.Core" ],
  "projectUrl": "https://github.com/mgravell/protobuf-net",
  "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
  "requireLicenseAcceptance": true,

  "compilationOptions": {
    "define": [ "TRACE", "FEAT_COMPILER", "FEAT_SERVICEMODEL", "PLAT_BINARYFORMATTER", "PLAT_XMLSERIALIZER" ],
    "keyFile": "../../tools/StrongKey.snk",
    "warningsAsErrors": false,
    "allowUnsafe": true
  },
  "compile": [
    "**/*.cs",
    "../../Common/*.cs"
  ],
  "compileExclude": [ "../../Common/CommonAssemblyInfo.cs" ],
  "exclude": [ ],
  "preprocess": [ ],
  "preprocessExclude": [ ],
  "resource": [ "**/*.resx" ],

  "dependencies": {
    "CuteAnt": "2.0.0-rc2-151206"
  },

  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime.Serialization": "4.0.0.0",
        "System.ServiceModel": "4.0.0.0"
      }
    }
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/mgravell/protobuf-net"
  }
}
