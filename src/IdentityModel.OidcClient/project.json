{
  "version": "2.0.0-rc3-160302",
  "description": "OpenID Connect Client Library for Native Applications",
  "authors": [ "Dominick Baier,  Brock Allen" ],
  "owners": [ "Dominick Baier,  Brock Allen" ],
  "tags": [ "OAuth2 OIDC OpenID Connect JWT Tokens Federation API Authorization" ],
  "iconUrl": "https://identityserver.github.io/Documentation/assets/images/icons/IDmodel_icon128.png",
  "projectUrl": "https://github.com/IdentityModel/IdentityModel.OidcClient",
  "licenseUrl": "https://github.com/IdentityModel/IdentityModel.OidcClient/blob/dev/LICENSE",
  "requireLicenseAcceptance": true,

  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE", "NET_3_5_GREATER" ],
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": false,
        "allowUnsafe": false,
        "nowarn": [ "CS1591" ]
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "TRACE", "NET_3_5_GREATER" ],
        "optimize": true,
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": false,
        "allowUnsafe": false,
        "nowarn": [ "CS1591" ],
        "xmlDoc": true
      }
    }
  },
  "compile": [ "**/*.cs" ],
  "compileExclude": [ ],
  "exclude": [ ],
  "preprocess": [ ],
  "preprocessExclude": [ ],
  "resource": [ "**/*.resx" ],

  "dependencies": {
    "IdentityModel": "2.0.0-rc3-*",
    "Newtonsoft.Json": "8.0.3",
    "jose-pcl": "0.0.4"
  },

  "frameworks": {
    "net451": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR", "NET_4_0_GREATER" ]
      },
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        }
      }
    },
    "dnx451": {
      "compilationOptions": {
        "define": [ "DNXCLR", "NET_4_0_GREATER" ]
      },
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        }
      }
    }
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/IdentityModel/IdentityModel.OidcClient"
  }
}
