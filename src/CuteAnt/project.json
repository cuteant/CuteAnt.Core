{
  "version": "2.0.0-rc3-160303",
  "description": "CuteAnt.Core Class Library",
  "authors": [ "SeaBiscuit" ],
  "tags": [ "CuteAnt", "CuteAnt.Core" ],
  "projectUrl": "http://www.cuteant.net",
  "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
  "requireLicenseAcceptance": true,

  "repository": {
    "type": "git",
    "url": "http://git.oschina.net/cuteant/"
  },

  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE", "NET_3_5_GREATER" ],
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": false,
        "allowUnsafe": true,
        "nowarn": [ "CS1591" ]
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "TRACE", "NET_3_5_GREATER" ],
        "optimize": true,
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": false,
        "allowUnsafe": true,
        "nowarn": [ "CS1591" ],
        "xmlDoc": true
      }
    }
  },
  "compile": [
    "**/*.cs",
    "../../Common/*.cs"
  ],
  "compileExclude": [ ],
  "exclude": [ ],
  "preprocess": [ ],
  "preprocessExclude": [ ],
  "resource": [ "**/*.resx" ],

  "dependencies": {
    "CuteAnt.Extensions.Reflection.Sources": {
      "type": "build",
      "version": "2.0.0-rc3-*"
    },
    "CuteAnt.Extensions.Sources": {
      "type": "build",
      "version": "2.0.0-rc3-*"
    },
    "CuteAnt.Extensions.Logging.Sources": {
      "type": "build",
      "version": "2.0.0-rc3-*"
    },
    "CuteAnt.Extensions.IO.Sources": {
      "type": "build",
      "version": "2.0.0-rc3-*"
    }
  },

  "frameworks": {
    "net40": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR" ]
      },
      "dependencies": {
        "CuteAnt.Extensions.Logging": "2.0.0-rc3-*"
      },
      "frameworkAssemblies": {
        "Microsoft.VisualBasic": "",
        "System.configuration": "",
        "System.Data": "",
        "System.Drawing": "",
        "System.Management": "",
        "System.Runtime.Serialization": "",
        "System.Security": "",
        "System.XML": "",
        "System.Xml.Linq": ""
      }
    },
    "net451": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR", "NET_4_0_GREATER" ]
      },
      "dependencies": {
        "CuteAnt.Extensions.Logging": "2.0.0-rc3-*"
      },
      "frameworkAssemblies": {
        "Microsoft.VisualBasic": "",
        "System.configuration": "",
        "System.Data": "",
        "System.Drawing": "",
        "System.Management": "",
        "System.Runtime.Serialization": "",
        "System.Security": "",
        "System.XML": "",
        "System.Xml.Linq": ""
      }
    }
  }
}
