  "dependencies": {
    "NLog": "4.2.2",
    "CuteAnt.Extensions.Sources": {
      "type": "build",
      "version": "2.0.0-*"
    },
    "CuteAnt.Extensions.IO.Sources": {
      "type": "build",
      "version": "2.0.0-*"
    }
  },

  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "Microsoft.VisualBasic": "10.0.0.0",
        "System.configuration": "4.0.0.0",
        "System.Data": "4.0.0.0",
        "System.Drawing": "4.0.0.0",
        "System.Management": "4.0.0.0",
        "System.Runtime.Serialization": "4.0.0.0",
        "System.Security": "4.0.0.0",
        "System.Web": "4.0.0.0",
        "System.Windows.Forms": "4.0.0.0",
        "System.XML": "4.0.0.0",
        "System.Xml.Linq": "4.0.0.0"
      }
    }
  },

  "repository": {
    "type": "git",
    "url": "http://git.oschina.net/cuteant/"
