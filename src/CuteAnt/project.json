  "requireLicenseAcceptance": false,

  "compilationOptions": {
    "define": [ "TRACE", "NET_4_5_X", "NET_3_5_GREATER", "NET_4_0_GREATER" ],
    "keyFile": "../../tools/StrongKey.snk",
    "warningsAsErrors": false,
    "allowUnsafe": true
  },
  "compile": [
    "**/*.cs",
    "../../Common/*.cs"
  ],
  "compileExclude": [ ],
  "exclude": [ ],
  "preprocess": [ ],
  "preprocessExclude": [ ],
