{
  "version": "2.0.0-rc3-160302",
  "description": "Helper library for claims-based identity, System.IdentityModel, JSON Web Tokens, OAuth 2.0 and OpenID Connect client libraries",
  "authors": [ "Brock Allen & Dominick Baier" ],
  "owners": [ "Brock Allen & Dominick Baier" ],
  "tags": [ "OAuth2 OIDC OpenID Connect JWT Tokens Federation API Authorization Security" ],
  "iconUrl": "https://identityserver.github.io/Documentation/assets/images/icons/IDmodel_icon128.png",
  "projectUrl": "https://github.com/IdentityModel/IdentityModel",
  "licenseUrl": "https://github.com/IdentityModel/IdentityModel/blob/master/LICENSE",
  "requireLicenseAcceptance": true,

  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE", "NET_3_5_GREATER" ],
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": false,
        "allowUnsafe": false,
        "nowarn": [ "CS1591" ]
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "TRACE", "NET_3_5_GREATER" ],
        "optimize": true,
        "keyFile": "../../tools/StrongKey.snk",
        "warningsAsErrors": false,
        "allowUnsafe": false,
        "nowarn": [ "CS1591" ],
        "xmlDoc": true
      }
    }
  },
  "compile": [ "**/*.cs" ],
  "compileExclude": [ ],
  "exclude": [ ],
  "preprocess": [ ],
  "preprocessExclude": [ ],
  "resource": [ "**/*.resx" ],

  "dependencies": {
    "Newtonsoft.Json": "8.0.3"
  },

  "frameworks": {
    "net451": {
      "compilationOptions": {
        "define": [ "DESKTOPCLR", "NET_4_0_GREATER" ]
      },
      "frameworkAssemblies": {
        "System.IdentityModel": "",
        "System.Net.Http": "",
        "System.XML": "",
        "System.Xml.Linq": ""
      }
    }
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/IdentityModel/IdentityModel"
  }
}
